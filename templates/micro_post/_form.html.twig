{{ form_start(form) }}
    <div>{{ form_errors(form)}}</div>

    <div>
        {{ form_label(form.title, 'Enter the title')}}
    </div>
    <div>
        {{ form_widget(form.title)}}
    </div>

    <div>
        {{ form_label(form.text, 'Enter the text')}}
    </div>
    <div>
        {{ form_widget(form.text)}}
    </div>

    <button type="submit">Save!</button>
{{ form_end(form)}}
